{
 "manifest_version": 2,
 "name": "Listen to Me",
 "version": "1",
 "description": "This extention allows you to control chrome using voice",

 "browser_action": {
 	"default_popup":"popup.html"
 },
 "background": {
    "scripts":["background.js"]
 },
 "content_scripts": [
    {
      "matches": ["https://www.google.com/*"],
      "js": ["jquery-2.1.3.min.js", "content.js"]
    }
 ],
 "permissions": [],
 "minimum_chrome_version": "28"
}